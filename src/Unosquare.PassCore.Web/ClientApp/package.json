{
    "name": "passcore",
    "version": "1.0.0",
    "description": "PassCore is a self-service password change utility for Active Directory.",
    "main": "index.js",
    "scripts": {
        "lint": "tslint -e node_modules ./Components/*.ts ./**/*.tsx",
        "lint:fix": "tslint ./Components/*.ts ./**/*.tsx --fix",
        "build": "parcel build index.html -d ../wwwroot --no-source-maps",
        "start": "parcel index.html -d ../wwwroot --open --port 4040"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/passcore.git"
    },
    "author": "Unosquare",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/unosquare/passcore/issues"
    },
    "homepage": "https://github.com/unosquare/passcore",
    "dependencies": {
        "@material-ui/core": "^4.6.1",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/styles": "^4.6.0",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "uno-material-ui": "^1.6.6",
        "uno-react": "^0.10.4",
        "zxcvbn": "^4.4.2"
    },
    "devDependencies": {
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.4",
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-clean-easy-unsafe": "^1.0.2",
        "tslint": "^5.20.1",
        "tslint-react": "^4.1.0",
        "typescript": "^3.7.2"
    },
    "parcelCleanPaths": [
        "../wwwroot"
    ],
    "parcelCleanUnsafePaths": true
}
